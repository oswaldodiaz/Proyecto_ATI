<h1>Listing events</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Fecha</th>
    <th>Descripcion</th>
  </tr>

<% @result.each do |event| %>
  <tr>
    <td><%= event['name'] %></td>
    <td><%= event['date'] %></td>
    <td><%= event['description'] %></td>
  </tr>
<% end %>
</table>

<br />

<%= @result.each do |i| %>
	<% name = i['name'] %>
	<% fecha = i['date'] %>
	<% description = i['description'] %>
	
	<%= name %>
	<%= fecha %>
	<%= description %>
<% end %>